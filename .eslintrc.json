{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "max-len": ["warn", { "code": 120 }],
    // "no-unused-vars": "warn",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always-and-inside-groups"
      }
    ]
  }
}
